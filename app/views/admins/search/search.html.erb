<div class="container">
  <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
      <h4>"<%= params[:search] %>"の検索結果</h4>

      <% if @which_option == "1" %>
        <%= render 'admins/customers/customers_list', customers: @customers %>
      <% end %>

      <% if @which_option == "2" %>
        <%= render 'admins/items/items_list', items: @items %>
      <% end %>

    </div>
  </div>
</div>