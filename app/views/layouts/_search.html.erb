<div class="form-inline pull-right">
  <%= form_with url: admins_search_url, method: :get, local: true do |f| %>
    <%= f.select :option, options_for_select([["会員", "1"],["商品","2"]]), class: "form-control" %>
    <%= f.text_field :search, placeholder: "キーワード検索", class: "form-control-sm" %>
    <%= f.submit "検索", class: "btn btn-info btn-sm"%>
  <% end %>
</div>
